{
  "name": "ky-vue-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build-test": "vue-cli-service build --mode test",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.18.0",
    "delegates": "^1.0.0",
    "iview": "^3.0.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "querystring": "^0.2.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.11",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.11",
    "@vue/cli-service": "^3.0.0-rc.11",
    "babel-plugin-import": "^1.8.0",
    "cross-env": "^5.2.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ],
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
